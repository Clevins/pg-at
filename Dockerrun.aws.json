{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "clevins/pg-at-client",
            "hostname": "client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "strapi",
            "image": "clevins/pg-at-strapi",
            "hostname": "strapi",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "clevins/pg-at-nginx",
            "essential": true,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": [
                "client",
                "strapi"
            ]
        }
    ]
}